  "special_instructions": {
    "adrp": {
      "description": "Address of Page - calculates page address",
      "behavior": "address_calculation_only"
    }
  },

  "memory_syntax": {
    "bracket_notation": "arm",
    "patterns": {
      "memory_operand": "\\[([^\\]]+)\\]",
      "immediate": "#([0-9]+)",
      "register": "\\b({register_names})\\b"
    }
  }
